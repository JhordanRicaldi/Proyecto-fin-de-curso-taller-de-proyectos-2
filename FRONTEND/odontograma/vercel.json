{
    "version": 2,
    "builds": [
      {
        "src": "dist/odontograma/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "dist/odontograma/browser/$1"
      },
      {
        "src": "/",
        "dest": "dist/odontograma/browser/index.html"
      }
    ]
  }